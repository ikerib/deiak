{% extends 'base.html.twig' %}

{% block body %}
    <h1>Deia</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ deium.id }}</td>
            </tr>
            <tr>
                <th>Userid</th>
                <td>{{ deium.userid }}</td>
            </tr>
            <tr>
                <th>Teknikoa</th>
                <td>{{ deium.teknikoa }}</td>
            </tr>
            <tr>
                <th>Azalpena</th>
                <td>{{ deium.azalpena }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if deium.createdAt %}{{ deium.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if deium.updatedAt %}{{ deium.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('deia_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('deia_edit', { 'id': deium.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
